{
  "name": "greenroofs",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://greenroofs.zaitra.io/",
  "scripts": {
    "build": "react-scripts build",
    "build:production": "env-cmd -f .env.production react-scripts build",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "predeploy": "react-scripts build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "eslint --fix --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.6",
    "@types/google-map-react": "^2.1.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-leaflet": "^2.5.0",
    "@types/reactstrap": "^8.4.1",
    "@types/recompose": "^0.30.7",
    "@types/shortid": "^0.0.29",
    "bootstrap": "^4.4.1",
    "env-cmd": "^10.0.1",
    "eslint-plugin-react": "^7.17.0",
    "gh-pages": "^3.1.0",
    "google-map-react": "^2.1.6",
    "i18next": "^19.0.2",
    "i18next-browser-languagedetector": "^6.0.1",
    "leaflet": "^1.6.0",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-google-maps": "^9.4.5",
    "react-i18next": "^11.2.6",
    "react-leaflet": "^2.6.1",
    "react-scripts": "3.4.3",
    "react-tooltip-lite": "^1.11.2",
    "reactstrap": "^8.4.1",
    "recompose": "^0.30.0",
    "shortid": "^2.2.15",
    "typescript": "~4.0.3",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@will-stone/prettier-config": "^3.1.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "prettier": "^2.1.2"
  }
}
